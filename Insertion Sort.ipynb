    "#insertion sort terkecil\n",
    "a = [5,2,4,6,1,3]\n",
    "for i in range (1,len(a)):\n",
    "    value = a[i]\n",
    "    hole = i\n",
    "    while (hole> 0 and a[hole-1] > value):\n",
    "        a[hole] = a[hole-1]\n",
    "        hole = hole-1\n",
    "    a[hole] = value\n",
    "    print(a)"
